{
	"name": "supplier_pipeline_synapse",
	"properties": {
		"activities": [
			{
				"name": "Copy data from parquet to sqlpool",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "ParquetSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "ParquetReadSettings"
						}
					},
					"sink": {
						"type": "SqlPoolSink",
						"allowCopyCommand": true,
						"tableOption": "autoCreate"
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "suppliers_syn_src",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "supplier_parquet_copy_to_synapse",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}